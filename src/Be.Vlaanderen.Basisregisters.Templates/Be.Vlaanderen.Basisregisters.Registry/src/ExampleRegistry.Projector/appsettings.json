{
  "ConnectionStrings": {
    "Events": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.ExampleRegistry;Trusted_Connection=True;",
    "BackofficeProjections": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.ExampleRegistry;Trusted_Connection=True;",
    "BackofficeProjectionsAdmin": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.ExampleRegistry;Trusted_Connection=True;",
    "LastChangedList": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.ExampleRegistry;Trusted_Connection=True;",
    "LastChangedListAdmin": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.ExampleRegistry;Trusted_Connection=True;"
  },

  "DataDog": {
    "Enabled": false,
    "Debug": false,
    "ServiceName": "example-registry-projector-api"
  },

  "Cors": [
    "http://localhost:3000",
    "http://localhost:5000"
  ],

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "ExampleRegistry": "Debug",
      "Be.Vlaanderen": "Debug",
      "Be.Vlaanderen.Basisregisters.DataDog.Tracing": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "ExampleRegistry": "Verbose",
        "Be.Vlaanderen": "Verbose",
        "Be.Vlaanderen.Basisregisters.DataDog.Tracing": "Information",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "Example Registry - Projector API"
    }
  }
}
